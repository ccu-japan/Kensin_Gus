<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CheckActivity"
    tools:layout_editor_absoluteY="89dp">

    <ScrollView
        android:id="@+id/scrollView2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="24dp"
        app:layout_constraintBottom_toTopOf="@+id/Check_OK"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center|top"
            android:orientation="vertical"
            android:scrollbarSize="4sp">

            <CheckBox
                android:id="@+id/Result1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:checked="false"
                android:ems="7"
                android:onClick="onCheckboxClicked"
                android:text="@string/Result1"
                android:textSize="44sp"
                tools:ignore="RtlCompat" />

            <CheckBox
                android:id="@+id/Result2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ems="7"
                android:onClick="onCheckboxClicked"
                android:text="@string/Result2"
                android:textSize="44sp" />

            <CheckBox
                android:id="@+id/Result3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ems="7"
                android:onClick="onCheckboxClicked"
                android:text="@string/Result3"
                android:textSize="44sp" />

            <CheckBox
                android:id="@+id/Result4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ems="7"
                android:onClick="onCheckboxClicked"
                android:text="@string/Result4"
                android:textSize="44sp" />

            <CheckBox
                android:id="@+id/Result5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ems="7"
                android:onClick="onCheckboxClicked"
                android:text="@string/Result5"
                android:textSize="44sp" />

            <CheckBox
                android:id="@+id/Result6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ems="7"
                android:onClick="onCheckboxClicked"
                android:text="@string/Result6"
                android:textSize="44sp" />

            <CheckBox
                android:id="@+id/Result7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ems="7"
                android:onClick="onCheckboxClicked"
                android:text="@string/Result7"
                android:textSize="44sp"
                tools:ignore="RtlCompat" />

            <CheckBox
                android:id="@+id/Result8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ems="7"
                android:onClick="onCheckboxClicked"
                android:text="@string/Result8"
                android:textSize="44sp" />

            <CheckBox
                android:id="@+id/Result9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ems="7"
                android:onClick="onCheckboxClicked"
                android:text="@string/Result9"
                android:textSize="44sp" />

            <CheckBox
                android:id="@+id/Result10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ems="7"
                android:onClick="onCheckboxClicked"
                android:text="@string/Result10"
                android:textSize="44sp" />

            <CheckBox
                android:id="@+id/Result11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ems="7"
                android:onClick="onCheckboxClicked"
                android:text="@string/Result11"
                android:textSize="44sp" />

            <CheckBox
                android:id="@+id/Result12"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ems="7"
                android:onClick="onCheckboxClicked"
                android:text="@string/Result12"
                android:textSize="44sp" />

        </LinearLayout>
    </ScrollView>

    <Button
        android:id="@+id/Check_Cancel"
        android:layout_width="175dp"
        android:layout_height="70dp"
        android:layout_marginBottom="25dp"
        android:text="@string/cancel"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/Check_OK"
        app:layout_constraintStart_toStartOf="@+id/Check_OK" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:text="@string/check_text"
        android:textColor="#000000"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <Button
        android:id="@+id/Check_OK"
        android:layout_width="175dp"
        android:layout_height="70dp"
        android:layout_marginBottom="25dp"
        android:text="@android:string/ok"
        android:textSize="25sp"
        app:layout_constraintBottom_toTopOf="@+id/Check_Cancel"
        app:layout_constraintEnd_toEndOf="@+id/textView5"
        app:layout_constraintStart_toStartOf="@+id/textView5"
        tools:ignore="RtlCompat" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="24dp"
        android:text="点検項目"
        android:textColor="#000000"
        android:textSize="44sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>