<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/root_search_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/frame_style"
    android:orientation="vertical">

    <Button
        android:id="@+id/Kensin_Button"
        android:layout_width="70dp"
        android:layout_height="34dp"
        android:layout_column="2"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:background="@drawable/frame_style"
        android:text="@string/search"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/customer_Button"
        android:layout_width="70dp"
        android:layout_height="34dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:background="@drawable/frame_style"
        android:text="@string/search"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Kensin_Button" />

    <Button
        android:id="@+id/Printer_Button"
        android:layout_width="70dp"
        android:layout_height="34dp"
        android:layout_column="3"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:background="@drawable/frame_style"
        android:text="@string/search"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/customer_Button" />

    <Button
        android:id="@+id/Calender_Button"
        android:layout_width="70dp"
        android:layout_height="34dp"
        android:layout_column="3"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:background="@drawable/frame_style"
        android:text="@string/search"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Printer_Button" />

    <TableLayout
        android:id="@+id/table1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        app:layout_constraintBottom_toTopOf="@+id/table2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="10dp">

            <TextView
                android:id="@+id/BlanckData"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/BlanckData"
                android:textSize="25sp"
                app:autoSizeMinTextSize="12sp"
                app:autoSizeTextType="uniform" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="10dp">

            <TextView
                android:id="@+id/CustomerCode"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/CustomerCode"
                android:textSize="25sp"
                app:autoSizeMinTextSize="12sp"
                app:autoSizeTextType="uniform" />

            <EditText
                android:id="@+id/customercode"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:background="@drawable/frame_style"
                android:ems="2"
                android:importantForAutofill="no"
                android:inputType="number"
                android:maxLength="4"
                android:textSize="25sp"
                tools:ignore="LabelFor"
                tools:targetApi="o" />

        </TableRow>

    </TableLayout>

    <TableLayout
        android:id="@+id/table2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/table1">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="10dp">

            <TextView
                android:id="@+id/GroupCode01"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/GroupCode"
                android:textSize="25sp"
                app:autoSizeMinTextSize="12sp"
                app:autoSizeTextType="uniform" />

            <EditText
                android:id="@+id/Code01"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/frame_style"
                android:ems="2"
                android:importantForAutofill="no"
                android:inputType="number"
                android:maxLength="3"
                android:textSize="25sp"
                tools:ignore="LabelFor"
                tools:targetApi="o" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/GroupCode02"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/GroupCode"
                android:textSize="25sp"
                app:autoSizeMinTextSize="12sp"
                app:autoSizeTextType="uniform" />

            <EditText
                android:id="@+id/Code02"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/frame_style"
                android:ems="2"
                android:importantForAutofill="no"
                android:inputType="number"
                android:maxLength="3"
                android:textSize="25sp"
                tools:ignore="LabelFor"
                tools:targetApi="o" />

            <EditText
                android:id="@+id/Code03"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:background="@drawable/frame_style"
                android:ems="2"
                android:importantForAutofill="no"
                android:inputType="number"
                android:maxLength="4"
                android:textSize="25sp"
                tools:ignore="LabelFor"
                tools:targetApi="o" />

        </TableRow>

    </TableLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/linearLayout2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="35dp"
        android:layout_marginEnd="10dp"
        android:background="@drawable/frame_style"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/table2">

        <TextView
            android:id="@+id/K_jun"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginTop="20dp"
            android:ems="3"
            android:textSize="24sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/G_code"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:ems="3"
            android:textSize="24sp"
            app:layout_constraintBottom_toBottomOf="@+id/K_jun"
            app:layout_constraintStart_toEndOf="@+id/K_jun"
            app:layout_constraintTop_toTopOf="@+id/K_jun" />

        <Button
            android:id="@+id/button"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="20dp"
            android:background="@drawable/frame_style"
            android:text="@string/mi"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:text="@string/kotei2"
            android:textSize="24sp"
            app:layout_constraintStart_toStartOf="@+id/K_jun"
            app:layout_constraintTop_toBottomOf="@+id/K_jun" />

        <TextView
            android:id="@+id/Name1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="8"
            android:textSize="24sp"
            app:layout_constraintBottom_toBottomOf="@+id/textView8"
            app:layout_constraintStart_toEndOf="@+id/textView8"
            app:layout_constraintTop_toTopOf="@+id/textView8" />

        <TextView
            android:id="@+id/Name2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="8"
            android:textSize="24sp"
            app:layout_constraintEnd_toEndOf="@+id/Name1"
            app:layout_constraintStart_toStartOf="@+id/Name1"
            app:layout_constraintTop_toBottomOf="@+id/Name1" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="50dp"
            android:layout_marginBottom="20dp"
            android:text="@string/kotei3"
            android:textSize="24sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="@+id/textView8"
            app:layout_constraintTop_toBottomOf="@+id/textView8" />

        <TextView
            android:id="@+id/CustomCode"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="2"
            android:textSize="24sp"
            app:layout_constraintBottom_toBottomOf="@+id/textView10"
            app:layout_constraintStart_toEndOf="@+id/textView10"
            app:layout_constraintTop_toTopOf="@+id/textView10" />

        <TextView
            android:id="@+id/P_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="6"
            android:textSize="24sp"
            app:layout_constraintBottom_toBottomOf="@+id/CustomCode"
            app:layout_constraintStart_toEndOf="@+id/CustomCode"
            app:layout_constraintTop_toTopOf="@+id/CustomCode" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <Button
        android:id="@+id/Up"
        android:layout_width="110dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:background="@drawable/frame_style"
        android:gravity="center"
        android:includeFontPadding="false"
        android:text="@string/down"
        android:textAlignment="center"
        android:textSize="40sp"
        app:layout_constraintEnd_toStartOf="@+id/OK"
        app:layout_constraintStart_toEndOf="@+id/Down"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout2"
        tools:ignore="ButtonStyle" />

    <Button
        android:id="@+id/Down"
        android:layout_width="110dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/frame_style"
        android:gravity="center"
        android:includeFontPadding="false"
        android:text="@string/up"
        android:textAlignment="center"
        android:textSize="40sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout2"
        tools:ignore="ButtonStyle" />

    <Button
        android:id="@+id/OK"
        android:layout_width="110dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:background="@drawable/frame_style"
        android:gravity="center"
        android:includeFontPadding="false"
        android:text="@android:string/VideoView_error_button"
        android:textAlignment="center"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout2"
        tools:ignore="ButtonStyle" />

</androidx.constraintlayout.widget.ConstraintLayout>